
@{
    ViewBag.Title = "Estadisticas";
    Layout = "~/Views/Shared/MasterComercio.cshtml";
}

<head>
    <title>Estadísticas</title>
</head>
<div class="w3-container" style="margin-top:10px" id="showcase">
    <h1 class="w3-justify"><b>Estadísticas</b></h1>
</div>




<div class="row gap-20 masonry pos-r" style="position: relative; height: 1856.82px;">
    <table id="datosretos" class="table table-striped table-bordered">
        <thead>
        <td>ID</td>
        <td>Reto</td>
        <td>Selección</td>
        </thead>
        <tbody id="contenedorpublis"></tbody>
    </table>
</div>




<script src="~/Scripts/admin_jquery_dist_jquery.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/jquery.simpleWeather/3.1.0/jquery.simpleWeather.min.js"></script>
<script src="~/Scripts/AccionesAdmin.js"></script>

<script>

    $(document).ready(function () {
        Verpublicidades();
    });

    function Verpublicidades(a) {
        $.ajax({
            url: "/Ajax/Ajax?data=verpublis&idpubli=" + a,
            type: "POST",
            success: function (a) {

                $("#contenedorpublis").html(a);

            }
        });
    };

</script>