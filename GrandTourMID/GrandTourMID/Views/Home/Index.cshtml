<!-- Container (Contact Section) -->
<div class="w3-content w3-container w3-padding-64" id="contact">
    <h3 class="w3-center" id="txtTitulo"></h3>
    <h3 class="w3-center" id="Subtitulo"><em></em></h3>
    <div class="w3-row w3-padding-32 w3-section">
        <div class="w3-col m4 w3-container">
            <!-- Add Google Maps -->
            <div id="googleMap" class="w3-round-large " style="width:100%;height:400px;"></div>
        </div>
        <div class="w3-col m8 w3-panel">
            <div class="w3-large w3-margin-bottom">
                <i class="fa fa-map-marker fa-fw w3-hover-text-black w3-xlarge " style="width:169px" id="direccion"></i> <br>
                <i class="fa fa-phone fa-fw w3-hover-text-black w3-xlarge w3-margin-right" id="telefono"></i> <br>
                <i class="fa fa-envelope fa-fw w3-hover-text-black w3-xlarge w3-margin-right" id="correo"></i> <br>
                <input type="hidden" class="w3-round w3-transparent" id="latitud" />
                <input type="hidden" class="w3-round w3-transparent w3-border" id="longitud" />
            </div>
            <p id="mesaage"><i class="fa fa-coffee"></i></p>
            <form id="frmMensajeContacto">
                <div class="w3-row-padding" style="margin:0 -16px 8px -16px">
                    <div class="w3-half">
                        <input class="w3-input w3-border" id="nomcontacto" type="text" placeholder="Nombre" name="nomcontacto">
                    </div>
                    <div class="w3-half">
                        <input class="w3-input w3-border" id="correocontacto" type="text" placeholder="Correo electronico" name="correocontacto">
                    </div>
                </div>
                <input class="w3-input w3-border" type="text" placeholder="Mensaje" id="mensajecontacto" name="mensajecontacto">
                <button class="w3-button w3-black w3-right w3-section" id="enviarcontacto"  type="submit">
                    <i class="fa fa-paper-plane"></i> Enviar Mensaje
                </button>
            </form>
        </div>
    </div>
</div>


